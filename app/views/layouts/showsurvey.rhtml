<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN"
   "http://www.w3.org/TR/html4/loose.dtd">
<% @user = current_user %>
<html>
<head>
	<meta http-equiv="Content-Type" content="text/html; charset=utf-8" >

	<title><%= @page_title %></title>
	<%= stylesheet_link_tag_html4 'cbcl' %>
	<%= stylesheet_link_tag_html4 'survey' %>
	<%= stylesheet_link_tag_html4 'validation' %>

	<%= javascript_include_tag 'prototype', :cache => true %>
	<%= javascript_include_tag 'effects', :cache => true %>
	<%= javascript_include_tag 'controls', :cache => true %>
	<%= javascript_include_tag 'application', :cache => true %>
	<%= javascript_include_tag 'wforms/wforms_core', :cache => true %>
	<%= javascript_include_tag 'wforms/wforms_switch', :cache => true %>

	<% javascript_include_tag 'wforms/wforms_jump', :cache => true %>
	<%= javascript_include_tag 'validation', :cache => true %>
	<%= javascript_include_tag 'tabbing', :cache => true %>
	<%= javascript_include_tag 'css_browser_selector', :cache => true %>
</head>
<body>

	<%= render :partial => 'layouts/info_header' %>

<div id="content_survey">
	<%= render :partial => 'layouts/error_messages' %>

	<%= @content_for_layout %>
	
	<div id="submitting" style="border: 1px solid green; padding: 8px; display: none;">Sender svar...</div>
</div>

</body>
</html>